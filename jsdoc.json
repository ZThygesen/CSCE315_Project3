{
    "plugins": ["plugins/markdown","better-docs/component"],
    "recurseDepth": 10,
    "source": {
      "include": ["front/src","routes/"],
      "includePattern": ".+\\.js(doc|x)?$",
      "excludePattern": "(node_modules/|docs)"
    },
    "templates": {
      "cleverLinks": true,
      "monospaceLinks": true,
      "better-docs": {
        "name": "My React components"
      }
    },
    "opts": {
      "destination": "./jsdoc",
      "recurse": true,
      "readme": "./README.md"
    }
  }